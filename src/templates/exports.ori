(api) => Tree.map(api.exports, (export) => Tree.indent`
  ${ export.kind === "class" ?  class.ori(export) : "" }
  ${ export.kind === "function" ? Tree.indent`
    <h3 id="${ export.name }">
      ${ export.static ? "static " : "" }
      ${ export.async ? "async " : "" }
      ${ export.generator ? "*" : "" }${ export.name }(${ parameters.ori(export.parameters) })
      ${ export.isConstructor ? "constructor" : "" }
    </h3>
    ${ function.ori(export) }
  ` : "" }
`)
