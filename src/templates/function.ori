(api) => indent`
  <ul>
  ${ map(api/parameters, (parameter) => indent`
    <li>${parameter/name}: ${ typeList.ori(parameter/types) }
      ${ parameter/description ? `&ndash; ${parameter/description}` : "" }
    </li>  
  `) }
  </ul>
  ${ !api/isConstructor
    ? indent`
      <p>
        Returns: ${ typeList.ori(api/returnTypes) }
      </p>
    `
    : "" }
  <p>
    ${ mdHtml(api/description) }
  </p>
`
